{
  "manifest_version": 2,

  "name": "CRATE Chrome Extension",
  "description": "",
  "version": "0.1.0",

  "permissions": [
    "http://*/",
    "https://*/",
    "storage"
  ],

  "icons": {
    "16": "img/crate_16x16.png",
    "48": "img/crate_48x48.png",
    "128": "img/crate_128x128.png"
  },

  "options_page": "settings.html",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "page": "background.html"
  }
}
